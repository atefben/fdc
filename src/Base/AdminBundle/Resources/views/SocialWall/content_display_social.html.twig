{% extends 'SonataAdminBundle:CRUD:base_list_field.html.twig' %}
{% trans_default_domain 'BaseAdminBundle' %}

{% block field %}
    <div>
        {% if object.content is not null %}
            <img src="{{ object.content }}" alt="" width="150">
        {% else %}
            <p width="150" style="vertical-align:middle; text-align: center;">{{ 'list.label_social_wall_content_none' | trans }}</p>
        {% endif %}
    </div>
{% endblock %}