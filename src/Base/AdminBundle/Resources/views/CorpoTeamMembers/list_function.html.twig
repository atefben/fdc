{% extends 'SonataAdminBundle:CRUD:base_list_field.html.twig' %}

{% block field %}
    <div>
        {% set translationLocale = object.findTranslationByLocale('fr') %}
        {% if translationLocale is not null %}
            {% if translationLocale.function is defined %}
                {{ translationLocale.function }}
            {% endif %}
        {% endif %}
    </div>
{% endblock %}