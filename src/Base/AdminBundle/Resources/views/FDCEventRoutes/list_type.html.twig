{% extends 'SonataAdminBundle:CRUD:base_list_field.html.twig' %}
{% trans_default_domain 'BaseAdminBundle' %}

{% block field %}
    <div>
        {% if object.type == constant('Base\\CoreBundle\\Entity\\FDCEventRoutes::MENU') %}
            <p style="float: left">Menu</p>
        {% elseif object.type == constant('Base\\CoreBundle\\Entity\\FDCEventRoutes::FOOTER') %}
            <p style="float: left">Footer</p>
        {% endif %}
    </div>
{% endblock %}