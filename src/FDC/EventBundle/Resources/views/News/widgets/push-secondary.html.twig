<div class="push-container white">
    <div class="container">

        {% if homepage.pushsSecondaryImage1 is null %}
            <div class="push"></div>
        {% else %}
            {% set trans = homepage.pushsSecondaryImage1 .findTranslationByLocale(app.request.locale) %}
            {% if trans is null or trans.file is null %}
                {% set trans = homepage.pushsSecondaryImage1 .findTranslationByLocale('fr') %}
            {% endif %}
            <div class="push">
                <div class="image-wrapper">
                    <img src="{% path trans.file , '325x205' %}" srcset="{% path trans.file , '325x205' %} 1x, {% path trans.file , '650x410' %} 2x" alt="{{ trans.alt }} ">
                </div>
                <div class="vCenter">
                    <div class="vCenterKid">
                        <h3>{{ homepage|trans_fallback('secondaryPushTitle1')|raw }}</h3>
                    </div>
                </div>
                <a {{ homepage|trans_fallback('secondaryPushUrl1')|external_link }} href="{{ homepage|trans_fallback('secondaryPushUrl1') }}"></a>
            </div>
        {% endif %}

