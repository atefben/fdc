{% trans_default_domain 'FDCEventBundle' %}
<meta property="og:url" content="{{ app.request.getUri }}" />
<meta property="og:type" content="article" />
<meta property="og:title" content="{{ news|trans_fallback('title') }}" />
<meta property="og:description" content="{{ news|trans_fallback('introduction')|slice(0, 30) }}..." />