{% trans_default_domain 'FDCEventBundle' %}
{% set festival_date = festival_date() %}

<!DOCTYPE html>
<html {{ sonata_seo_html_attributes() }} lang="{{ app.request.locale }}">
    <head {{ sonata_seo_head_attributes() }}>
        {{ sonata_seo_title() }}
        <meta name="viewport" content="width=1024px, initial-scale=0.4">
        {{ sonata_seo_metadatas() }}
        {{ sonata_seo_link_canonical() }}
        {{ sonata_seo_lang_alternates() }}
        {% include 'FDCEventBundle:Base:stylesheet.html.twig' %}
        {% include 'FDCEventBundle:Base:javascript_head.html.twig' %}
    </head>
    <body>
    {% block pre_home %}
        {% endblock %}
        {% include 'FDCEventBundle::header.html.twig' %}
        {% block body %}
        {% endblock %}
        {% include 'FDCEventBundle:Global:cookies.html.twig' %}
        {% include 'FDCEventBundle::footer.html.twig' %}
        {% include 'FDCEventBundle:Base:javascript_footer.html.twig' %}
        {% include 'FDCEventBundle:Base:javascript.html.twig' %}
    </body>
</html>