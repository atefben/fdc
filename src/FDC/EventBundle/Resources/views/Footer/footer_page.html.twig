{% trans_default_domain 'FDCEventBundle' %}
{% extends 'FDCEventBundle::layout.html.twig' %}

{% block body %}
    <div id="main" class="legal-notice single-article">
        <h2 class="title">{{ page|trans_fallback('title') }}</h2>
        <section class="legal-notice-container">
            {{ page|trans_fallback('content')|raw }}
        </section>
    </div>
{% endblock body %}