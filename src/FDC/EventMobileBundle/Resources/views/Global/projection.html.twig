{%- spaceless -%}
{%- trans_default_domain 'FDCEventBundle' -%}
<div class="v-wrapper-content list">
    {%- for room in rooms -%}
    <div class="venue">
        <div class="v-head">{{ room.name }}</div>
        <div class="v-container">
            {%- for event in projections[room.id] -%}
                {%- include 'FDCEventMobileBundle:Agenda:widgets/event.html.twig' -%}
            {%- endfor -%}
        </div>
    </div>
    {%- endfor -%}
</div>
{%- endspaceless -%}