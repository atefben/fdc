{% trans_default_domain 'FDCEventBundle' %}
{% spaceless %}
    {% if class(object) == 'Event' %}
        {{ path('fdc_event_event_get', {'slug': object|trans_fallback('slug') }) }}
    {% elseif class(object) starts with 'Info' %}
        {{ path('fdc_press_news_get', {'format': object.newsFormat, 'slug': object|trans_fallback('slug'), 'type': 'info' }) }}
    {% elseif class(object) starts with 'Statement' %}
        {{ path('fdc_press_news_get', {'format': object.newsFormat, 'slug': object|trans_fallback('slug'), 'type': 'communique' }) }}
    {% elseif class(object) == 'MediaAudio' %}
            {{ path('fdc_event_news_getaudios') ~ '#aid=' ~ object.id }}
    {% elseif class(object) == 'MediaVideo' %}
            {{ path('fdc_event_news_getvideos') ~ '#vid=' ~ object.id }}
    {% elseif class(object) == 'MediaImage' %}
            {{ path('fdc_event_news_getphotos') ~ '#pid=' ~ object.id }}
    {% elseif class(object) == 'FDCPageParticipate' %}
        {{ path('fdc_event_participate_getpage', {'slug': object|trans_fallback('slug') }) }}
    {% elseif class(object) starts with 'News' %}
        {{ path('fdc_event_news_get', {'format': object.newsFormat, 'slug': object|trans_fallback('slug')}) }}
    {% elseif class(object) == 'FilmFilm' %}
        {{ path('fdc_event_movie_get', {'slug': object.slug}) }}
    {% elseif class(object) == 'FilmPerson' %}
        {{ path('fdc_eventmobile_artist_get', {'slug': object.slug }) }}
    {% endif %}
{% endspaceless %}