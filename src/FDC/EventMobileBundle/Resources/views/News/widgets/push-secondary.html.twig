{% if homepage.pushsSecondaryImage1 is not null %}
<div class="item">
    <div class="image-wrapper">
        {% if homepage.pushsSecondaryImage1 is not null %}
            {% set trans = homepage.pushsSecondaryImage1.findTranslationByLocale(app.request.locale) %}
            {% if trans is null or trans.file is null %}
                {% set trans = homepage.pushsSecondaryImage1.findTranslationByLocale('fr') %}
            {% endif %}
            <img class="img" src="{% path trans.file, '650x410' %}" alt="{{ trans.alt }}" />
        {% endif %}
    </div>
    <a class="linkVid" href="{{ homepage|trans_fallback('secondaryPushUrl1') }}"></a>
    <div class="info">
        <h3 class="category">{{ homepage|trans_fallback('secondaryPushTitle1') }}</h3>
    </div>
</div>
{% endif %}
