{% trans_default_domain 'FDCPressBundle' %}

<div class="venue">
    <div class="v-head">{{ 'press.home.salle.grandtheatrelumière'|trans }}</div>
    <div class="v-container">
        {% if dayProjection is defined %}
            {% for p in dayProjection %}
                {% if p.projection.room.name == "Grand Théatre Lumière" %}
                    {% if class(p) == "PressProjectionScheduling" %}
                        {% include 'FDCPressBundle:Agenda:widgets/event.html.twig' %}
                    {% elseif class(p) == "PressProjectionPressScheduling"  %}
                        {% include 'FDCPressBundle:Agenda:widgets/event-press.html.twig' %}
                    {% endif %}
                {% endif %}
            {% endfor %}
        {% endif %}
    </div>
</div>
