{% block field %}

<tr>
    <th>
        Medias
    </th>
    <td>
        <table class="table table-bordered">
            <thead>
                <tr class="sonata-ba-list-field-header">
                    <th class="text-center">
                        Identifiant
                    </th>
                    <th class="text-center">
                        Media
                    </th>
                    <th class="text-center">
                        Position
                    </th>
                </tr>
            </thead>
            <tbody>
                {% for filmMedia in object.medias %}
                <tr>
                    <td class="text-center">
                        <a target="_blank" href="{{ path('admin_fdc_core_filmmedia_show', {id: filmMedia.media.id }) }}">
                            {{ filmMedia.media.id }}
                        </a>
                    </td>
                    <td class="text-center">
                        <div>
                            {% media filmMedia.media.file, 'small' %}
                        </div>
                    </td>
                    <td class="text-center">
                        {{ filmMedia.position }}
                    </td>
                </tr>
                {% endfor %}
            </tbody>
        </table>
    </td>
</tr>
{% endblock %}