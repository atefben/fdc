                                    <figcaption>{% if copyright is not empty %} ¬© {{ copyright }}{% endif %}</figcaption>
                                </figure>
                            </div>
                        {% endfor %}
                    </div>

                    <div class="block-legend">
                        <div class="info">

                            <a href="#" class="category">{{ homepage.gallery.themeHomeCorpo|trans_fallback('name') }} </a><span class="date">{{ homepage.gallery.dateHomeCorpo|date('d.m.y') }}</span> . <span class="hour">{{ homepage.gallery.dateHomeCorpo|date('H:i') }}</span>

                            <div class="texts">
                                <h2><a href="article.html">{{ homepage.gallery|trans_fallback('titleHomeCorpo') }}</a></h2>
                                <div class="text-01">
                                    {{ homepage.gallery|trans_fallback('introductionHomeCorpo')|raw }}
                                </div>
                            </div> <!-- end block texts-->

                        </div> <!-- end block info -->
                    </div> <!-- end block legend-->

                </div> <!-- end diaporama-->
                {% endif %}

