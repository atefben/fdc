{% for push in pushes %}
    <div class="item">
        <div class="title">
            <a href=""><h5>{{ push.whiteTitle }}</h5></a>
            <a href=""><h4>{{ push.goldenTitle }}</h4></a>
        </div>
        <a href="{{ push.url }}" {% if fdc_court_metrage_scheme not in push.url %}target="_blank"{% endif %}><img style="object-fit: cover; width: 618px; height: 473px" src="{% path push.translatable.image|trans_fallback('file'), 'reference' %}"></a>
    </div>
{% endfor %}
