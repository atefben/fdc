{% set _preview      = block('preview') %}
{% set _form         = block('form') %}
{% set _show         = block('show') %}
{% set _list_table   = block('list_table') %}
{% set _list_filters = block('list_filters') %}
{% set _tab_menu     = block('tab_menu') %}
{% set _content      = block('content') %}
{% set _title        = block('title') %}
{% set _breadcrumb   = block('breadcrumb') %}
{% set _actions      = block('actions') %}
{% set _navbar_title = block('navbar_title') %}
<!DOCTYPE html>
<html {% block html_attributes %}class="no-js"{% endblock %}>
<head>
    {% block meta_tags %}
        <meta charset="UTF-8">
        <meta content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no' name='viewport'>
    {% endblock %}

    {% block stylesheets %}
        {% stylesheets
        '@BaseAdminBundle/Resources/public/css/*.css' %}

        <link rel="stylesheet" href="{{ asset_url }}"/>
        {% endstylesheets %}

        {% for stylesheet in admin_pool.getOption('stylesheets', []) %}
            <link rel="stylesheet" href="{{ asset(stylesheet) }}">
        {% endfor %}

        {# FDC CUSTOM CSS #}
        <link rel="stylesheet" href="//code.jquery.com/ui/1.11.4/themes/smoothness/jquery-ui.css">

    {% endblock %}

    {% block javascripts %}
        <script>
            window.SONATA_CONFIG = {
                CONFIRM_EXIT: {% if admin_pool is defined and admin_pool.getOption('confirm_exit') %}true{% else %}false{% endif %},
                USE_SELECT2: {% if admin_pool is defined and admin_pool.getOption('use_select2') %}true{% else %}false{% endif %},
                USE_ICHECK: {% if admin_pool is defined and admin_pool.getOption('use_icheck') %}true{% else %}false{% endif %}
            };
            window.SONATA_TRANSLATIONS = {
                CONFIRM_EXIT: '{{ 'confirm_exit'|trans({}, 'SonataAdminBundle')|escape('js') }}'
            };
        </script>

        {% for javascript in admin_pool.getOption('javascripts', []) %}
        <script src="{{ asset(javascript) }}"></script>
        {% endfor %}

        {# localize select2 #}
        {% if admin_pool is defined and admin_pool.getOption('use_select2') %}
        {% set locale = app.request.locale %}
        {% if locale == 'pt' %}{% set locale = 'pt_PT' %}{% endif %}

        {# omit default EN locale #}
        {% if locale[:2] != 'en' %}
        <script src="{{ asset('bundles/sonataadmin/vendor/select2/select2_locale_' ~ locale|replace({'_':'-'}) ~ '.js') }}"></script>
        {% endif %}
        {% endif %}

        {# FDC CUSTOM JAVASCRIPTS #}
        <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
        <script src="{{ path('fos_js_routing_js', {'callback': 'fos.Router.setData'}) }}"></script>

    {% endblock %}

    {% set userSite = app.session.get('base_site') %}
    {% if userSite is not null %}
        <script type="text/javascript">
            jQuery(document).ready(function () {
                jQuery('header.header').addClass('main-header');
                jQuery('header.header .logo span').text('{{ userSite.name }} Administration');
                jQuery('body').removeClass('skin-black').addClass('skin-{{ userSite.classColor }}');
            });
        </script>
    {% endif %}

    <title>
        {% block sonata_head_title %}
            {{ 'Admin'|trans({}, 'SonataAdminBundle') }}

            {% if _title is not empty %}
                {{ _title|raw }}
            {% else %}
                {% if action is defined %}
                    -
                    {% for menu in admin.breadcrumbs(action) %}
                        {% if not loop.first %}
                            {% if loop.index != 2 %}
                                &gt;
                            {% endif %}

                            {{ menu.label }}
                        {% endif %}
                    {% endfor %}
                {% endif %}
            {% endif %}
        {% endblock %}
    </title>
</head>
<body {% block body_attributes %}class="sonata-bc skin-black fixed{% if app.request.get('_route') in admin_routes_translation_lock %} fdc-lock{% endif %}{% if is_granted('ROLE_TRANSLATOR') %} role_translator{% endif %}"{% endblock %}>
{% include 'BaseAdminBundle:Site:modal.html.twig' %}
{# custom dialog #}
<div id="fdc-dialog">
    <div id="fdc-dialog-text">
    </div>
</div>
{% block sonata_header %}
    <header class="header">
        {% block sonata_header_noscript_warning %}
            <noscript>
                <div class="noscript-warning">
                    {{ 'noscript_warning'|trans({}, 'SonataAdminBundle') }}
                </div>
            </noscript>
        {% endblock %}
        {% block logo %}
            {% spaceless %}
                <a class="logo" href="{{ url('sonata_admin_dashboard') }}">
                    {% if 'single_image' == admin_pool.getOption('title_mode') or 'both' == admin_pool.getOption('title_mode') %}
                        <img src="{{ asset(admin_pool.titlelogo) }}" alt="{{ admin_pool.title }}">
                    {% endif %}
                    {% if 'single_text' == admin_pool.getOption('title_mode') or 'both' == admin_pool.getOption('title_mode') %}
                        <span>{{ admin_pool.title }}</span>
                    {% endif %}
                </a>
            {% endspaceless %}
        {% endblock %}
        {% block sonata_nav %}
            {% if admin_pool is defined %}
                <nav class="navbar navbar-static-top" role="navigation">
                    <a href="#" class="navbar-btn sidebar-toggle" data-toggle="offcanvas" role="button">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </a>

                    <div class="navbar-left">
                        {% block sonata_breadcrumb %}
                            {% if _breadcrumb is not empty or action is defined %}
                                <ol class="nav navbar-top-links breadcrumb">
                                    {% if _breadcrumb is empty %}
                                        {% if action is defined %}
                                            {% for menu in admin.breadcrumbs(action) %}
                                                {% if not loop.last %}
                                                    <li>
                                                        {% if menu.uri is not empty %}
                                                            <a href="{{ menu.uri }}">{{ menu.label|raw }}</a>
                                                        {% else %}
                                                            {{ menu.label }}
                                                        {% endif %}
                                                    </li>
                                                {% else %}
                                                    <li class="active"><span>{{ menu.label }}</span></li>
                                                {% endif %}
                                            {% endfor %}
                                        {% endif %}
                                    {% else %}
                                        {{ _breadcrumb|raw }}
                                    {% endif %}
                                </ol>
                            {% endif %}
                        {% endblock sonata_breadcrumb %}
                    </div>

                    {% block sonata_top_nav_menu %}
                        <div class="navbar-right">
                            <ul class="nav navbar-nav">
                                <li class="dropdown">
                                    <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                                        <i class="fa fa-plus-square fa-fw"></i> <i class="fa fa-caret-down"></i>
                                    </a>
                                    {% include admin_pool.getTemplate('add_block') %}
                                </li>
                                <li class="dropdown user-menu">
                                    <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                                        <i class="fa fa-user fa-fw"></i> <i class="fa fa-caret-down"></i>
                                    </a>
                                    <ul class="dropdown-menu dropdown-user">
                                        <div style="padding: 10px;">
                                            {% include admin_pool.getTemplate('user_block') %}
                                        </div>
                                    </ul>
                                </li>
                            </ul>
                            {% include 'BaseAdminBundle:Site:btn_modal.html.twig' %}
                        </div>
                    {% endblock %}
                </nav>
            {% endif %}
        {% endblock sonata_nav %}
    </header>
{% endblock sonata_header %}

{% block sonata_wrapper %}
    <div class="wrapper row-offcanvas row-offcanvas-left">
        {% block sonata_left_side %}
            <aside class="left-side sidebar-offcanvas">
                <section class="sidebar">
                    {% block sonata_side_nav %}
                        {% block sonata_sidebar_search %}
                            {% if app.security.token and is_granted('ROLE_SONATA_ADMIN') %}
                                <form action="{{ url('sonata_admin_search') }}" method="GET" class="sidebar-form"
                                      role="search">
                                    <div class="input-group custom-search-form">
                                        <input type="text" name="q" value="{{ app.request.get('q') }}"
                                               class="form-control"
                                               placeholder="{{ 'search_placeholder'|trans({}, 'SonataAdminBundle') }}">
                                                    <span class="input-group-btn">
                                                        <button class="btn btn-flat" type="submit">
                                                            <i class="fa fa-search"></i>
                                                        </button>
                                                    </span>
                                    </div>
                                </form>
                            {% endif %}
                        {% endblock sonata_sidebar_search %}

                        {% block side_bar_before_nav %} {% endblock %}
                        {% block side_bar_nav %}
                            {% if app.security.token and is_granted('ROLE_SONATA_ADMIN') %}
                                <ul class="sidebar-menu">

                                    {% if is_granted('ROLE_BASE_ADMIN_CCM_SHORT_FILM_COMPETITION_TAB_SELECTION_ADMIN') %}
                                        {% set active = app.request.attributes.get('_route') in
                                        ['ccm_competition_selection_edit',
                                        'ccm_competition_jury_edit',
                                        'ccm_competition_palmares_edit'] ? true : false %}
                                        <li class="treeview {% if active %} active{% endif %}">
                                            <a href="#">
                                                <i class="fa fa-folder"></i>
                                                <span>{{ 'admin.group.ccm.seo_tetieres'|trans({}, 'BaseAdminBundle') }}</span>
                                                <i class="fa pull-right fa-angle-left"></i>
                                            </a>
                                            <ul class="treeview-menu {% if active %} active{% endif %}">
                                                {% if is_granted('ROLE_BASE_ADMIN_CCM_SHORT_FILM_COMPETITION_TAB_SELECTION_ADMIN') %}
                                                    <li class="{{ app.request.attributes.get('_route') == 'ccm_competition_selection_edit' ? 'active' }}">
                                                        <a data-edit="true"
                                                           href="{{ path('ccm_competition_selection_edit', {id: 1}) }}">
                                                            <i class="fa fa-angle-double-right"></i>
                                                            {{ 'admin.ccm.menu.selection'|trans({}, 'BaseAdminBundle') }}
                                                        </a>
                                                    </li>
                                                {% endif %}
                                                {% if is_granted('ROLE_BASE_ADMIN_CCM_SHORT_FILM_COMPETITION_TAB_JURY_ADMIN') %}
                                                    <li class="{{ app.request.attributes.get('_route') == 'ccm_competition_jury_edit' ? 'active' }}">
                                                        <a data-edit="true"
                                                           href="{{ path('ccm_competition_jury_edit', {id: 2}) }}">
                                                            <i class="fa fa-angle-double-right"></i>
                                                            {{ 'admin.ccm.menu.jury'|trans({}, 'BaseAdminBundle') }}
                                                        </a>
                                                    </li>
                                                {% endif %}
                                                {% if is_granted('ROLE_BASE_ADMIN_CCM_SHORT_FILM_COMPETITION_TAB_PALMARES_ADMIN') %}
                                                    <li class="{{ app.request.attributes.get('_route') == 'ccm_competition_palmares_edit' ? 'active' }}">
                                                        <a data-edit="true"
                                                           href="{{ path('ccm_competition_palmares_edit', {id: 3}) }}">
                                                            <i class="fa fa-angle-double-right"></i>
                                                            {{ 'admin.ccm.menu.palmares'|trans({}, 'BaseAdminBundle') }}
                                                        </a>
                                                    </li>
                                                {% endif %}
                                            </ul>
                                        </li>
                                    {% endif %}

                                    {% if is_granted('ROLE_BASE_ADMIN_CCM_FAQ_PAGE_ADMIN') %}
                                        {% set active = app.request.attributes.get('_route') in
                                        ['admin_fdc_courtmetrage_ccmfaqpage_edit',
                                        'admin_fdc_courtmetrage_ccmcontactpage_edit',
                                        'ccm_confidentialite_edit',
                                        'ccm_credits_edit',
                                        'ccm_legal_mentions_edit'] ? true : false %}
                                        <li class="treeview {% if active %} active{% endif %}">
                                            <a href="#">
                                                <i class="fa fa-folder"></i>
                                                <span>{{ 'admin.group.ccm.footer'|trans({}, 'BaseAdminBundle') }}</span>
                                                <i class="fa pull-right fa-angle-left"></i>
                                            </a>
                                            <ul class="treeview-menu {% if active %} active{% endif %}">
                                                {% if is_granted('ROLE_BASE_ADMIN_CCM_FAQ_PAGE_ADMIN') %}
                                                    <li class="{{ app.request.attributes.get('_route') == 'admin_fdc_courtmetrage_ccmfaqpage_edit' ? 'active' }}">
                                                        <a data-edit="true"
                                                           href="{{ path('admin_fdc_courtmetrage_ccmfaqpage_edit', {id: 1}) }}">
                                                            <i class="fa fa-angle-double-right"></i>
                                                            {{ 'admin.ccm.menu.faq'|trans({}, 'BaseAdminBundle') }}
                                                        </a>
                                                    </li>
                                                {% endif %}
                                                {% if is_granted('ROLE_BASE_ADMIN_CCM_CONTACT_PAGE_ADMIN') %}
                                                    <li class="{{ app.request.attributes.get('_route') == 'admin_fdc_courtmetrage_ccmcontactpage_edit' ? 'active' }}">
                                                        <a data-edit="true"
                                                           href="{{ path('admin_fdc_courtmetrage_ccmcontactpage_edit', {id: 1}) }}">
                                                            <i class="fa fa-angle-double-right"></i>
                                                            {{ 'admin.ccm.menu.contact_page'|trans({}, 'BaseAdminBundle') }}
                                                        </a>
                                                    </li>
                                                {% endif %}
                                                {% if is_granted('ROLE_BASE_ADMIN_CCM_CONFIDENTIALITE_ADMIN') %}
                                                    <li class="{{ app.request.attributes.get('_route') == 'ccm_confidentialite_edit' ? 'active' }}">
                                                        <a data-edit="true"
                                                           href="{{ path('ccm_confidentialite_edit', {id: 3}) }}">
                                                            <i class="fa fa-angle-double-right"></i>
                                                            {{ 'admin.ccm.menu.confidentialite'|trans({}, 'BaseAdminBundle') }}
                                                        </a>
                                                    </li>
                                                {% endif %}
                                                {% if is_granted('ROLE_BASE_ADMIN_CCM_CREDITS_ADMIN') %}
                                                    <li class="{{ app.request.attributes.get('_route') == 'ccm_credits_edit' ? 'active' }}">
                                                        <a data-edit="true"
                                                           href="{{ path('ccm_credits_edit', {id: 2}) }}">
                                                            <i class="fa fa-angle-double-right"></i>
                                                            {{ 'admin.ccm.menu.credits'|trans({}, 'BaseAdminBundle') }}
                                                        </a>
                                                    </li>
                                                {% endif %}
                                                {% if is_granted('ROLE_BASE_ADMIN_CCM_LEGAL_ADMIN') %}
                                                    <li class="{{ app.request.attributes.get('_route') == 'ccm_legal_mentions_edit' ? 'active' }}">
                                                        <a data-edit="true"
                                                           href="{{ path('ccm_legal_mentions_edit', {id: 1}) }}">
                                                            <i class="fa fa-angle-double-right"></i>
                                                            {{ 'admin.ccm.menu.legal_mentions'|trans({}, 'BaseAdminBundle') }}
                                                        </a>
                                                    </li>
                                                {% endif %}
                                            </ul>
                                        </li>
                                    {% endif %}

                                    {% for group in admin_pool.dashboardgroups if group.label != 'sonata_media' %}
                                        {% set display = (group.roles is empty or is_granted('ROLE_SUPER_ADMIN') ) %}
                                        {% for role in group.roles if not display %}
                                            {% set display = is_granted(role) %}
                                        {% endfor %}

                                        {# Do not display the group label if no item in group is available #}
                                        {% set item_count = 0 %}
                                        {% if display %}
                                            {% for admin in group.items if item_count == 0 %}
                                                {% if admin.hasroute('list') and admin.isGranted('EDIT') %}
                                                    {% set item_count = item_count+1 %}
                                                {% endif %}
                                            {% endfor %}
                                        {% endif %}
                                        {% if group.label == 'dashboard.group.soif' and is_granted('ROLE_ADMIN_CCM') and not is_granted('ROLE_ALL_ADMIN') %}
                                            {% set display = false %}
                                        {% endif %}
                                        {% if display and (item_count > 0) and group.label != "dashboard.group.participate" and group.label != "sonata_user" %}
                                            {% set active = false %}
                                            {% for admin in group.items %}
                                                {% if admin.hasroute('list') and admin.isGranted('EDIT') and app.request.get('_sonata_admin') == admin.code %}
                                                    {% set active = true %}
                                                {% endif %}
                                            {% endfor %}
                                            {% set active = group.label == 'admin.group.ccm.pros' and app.request.attributes.get('_route') in
                                                ['admin_fdc_courtmetrage_ccmprospage_edit',
                                                'admin_fdc_courtmetrage_ccmprosdetail_list',
                                                'admin_fdc_courtmetrage_ccmprosdetail_create'] ? true : active %}
                                            {% set active = group.label == 'admin.group.ccm.general' and app.request.attributes.get('_route') in
                                                ['admin_fdc_courtmetrage_ccmmenu_edit',
                                                'admin_fdc_courtmetrage_homepage_edit',
                                                'admin_fdc_courtmetrage_ccmsocialwallhashtag_edit'] ? true : active %}
                                            {% set active = group.label == 'admin.group.ccm.participer' and app.request.attributes.get('_route') in
                                                ['admin_fdc_courtmetrage_ccmfilmregister_edit',
                                                'admin_fdc_courtmetrage_ccmlabel_edit',
                                                'ccm_plan_des_sales_edit',
                                                'admin_fdc_courtmetrage_ccmregisterprocedure_list',
                                                'admin_fdc_courtmetrage_ccmparticiperpage_list',
                                                'admin_fdc_courtmetrage_ccmparticiperpagelayer_list'] ? true : active %}
                                            {% set active = group.label == 'admin.group.ccm.short' and app.request.attributes.get('_route') in
                                                ['admin_fdc_courtmetrage_ccmprogram_edit',
                                                'ccm_sfc_who_are_we_list',
                                                'ccm_sfc_our_events_list',
                                                'ccm_sfc_relive_edition_list'] ? true : active %}
                                            <li class="treeview{% if active %} active{% endif %}">
                                                <a href="#">
                                                    {% if group.icon|default() %}{{ group.icon|raw }}{% endif %}
                                                    <span>{{ group.label|trans({}, group.label_catalogue) }}</span>
                                                    <i class="fa pull-right fa-angle-left"></i>
                                                </a>
                                                <ul class="treeview-menu active">
                                                    {% if group.label == 'admin.group.ccm.pros' %}
                                                        {% if is_granted('ROLE_BASE_ADMIN_CCM_PROS_PAGE_ADMIN') %}
                                                            <li class="{{ app.request.attributes.get('_route') == 'admin_fdc_courtmetrage_ccmprospage_edit' ? 'active' }}">
                                                                <a data-edit="true"
                                                                   href="{{ path('admin_fdc_courtmetrage_ccmprospage_edit', {id: 1}) }}">
                                                                    <i class="fa fa-angle-double-right"></i>
                                                                    {{ 'admin.ccm.menu.pros'|trans({}, 'BaseAdminBundle') }}
                                                                </a>
                                                            </li>
                                                        {% endif %}
                                                    {% endif %}

                                                    {% if group.label == 'admin.group.ccm.general' %}
                                                        {% if is_granted('ROLE_BASE_ADMIN_CCM_MENU_ADMIN') %}
                                                            <li class="{{ app.request.attributes.get('_route') == 'admin_fdc_courtmetrage_ccmmenu_edit' ? 'active' }}">
                                                                <a data-edit="true"
                                                                   href="{{ path('admin_fdc_courtmetrage_ccmmenu_edit', {id: 1}) }}">
                                                                    <i class="fa fa-angle-double-right"></i>
                                                                    {{ 'admin.ccm.menu.menu'|trans({}, 'BaseAdminBundle') }}
                                                                </a>
                                                            </li>
                                                        {% endif %}
                                                        {% if is_granted('ROLE_BASE_ADMIN_CCM_HOMEPAGE_ADMIN') %}
                                                            <li class="{{ app.request.attributes.get('_route') == 'admin_fdc_courtmetrage_homepage_edit' ? 'active' }}">
                                                                <a data-edit="true"
                                                                   href="{{ path('admin_fdc_courtmetrage_homepage_edit', {id: 1}) }}">
                                                                    <i class="fa fa-angle-double-right"></i>
                                                                    {{ 'admin.ccm.menu.homepage'|trans({}, 'BaseAdminBundle') }}
                                                                </a>
                                                            </li>
                                                        {% endif %}
                                                        {% if is_granted('ROLE_BASE_ADMIN_CCM_SOCIAL_WALL_HASHTAG_ADMIN') %}
                                                            <li class="{{ app.request.attributes.get('_route') == 'admin_fdc_courtmetrage_ccmsocialwallhashtag_edit' ? 'active' }}">
                                                                <a data-edit="true"
                                                                   href="{{ path('admin_fdc_courtmetrage_ccmsocialwallhashtag_edit', {id: 1}) }}">
                                                                    <i class="fa fa-angle-double-right"></i>
                                                                    {{ 'admin.ccm.menu.social_wall_hashtag'|trans({}, 'BaseAdminBundle') }}
                                                                </a>
                                                            </li>
                                                        {% endif %}
                                                        {% if is_granted('ROLE_SONATA_USER_ADMIN_USER_ADMIN') %}
                                                            <li>
                                                                <a href="{{ path('admin_sonata_user_user_list') }}">
                                                                    <i class="fa fa-angle-double-right"></i>
                                                                    {{ 'dashboard.link.users'|trans({}, 'BaseAdminBundle') }}
                                                                </a>
                                                            </li>
                                                        {% endif %}
                                                        {% if app.security.token and is_granted('ROLE_JMS_TRANSLATION') %}
                                                            <li>
                                                                <a data-edit="true"
                                                                   href="{{ path('jms_translation_index') }}">
                                                                    <i class="fa fa-angle-double-right"></i>
                                                                    {{ 'dashboard.link.bo_translation'|trans({}, 'BaseAdminBundle') }}
                                                                </a>
                                                            </li>
                                                        {% endif %}
                                                    {% endif %}
                                                    {% if group.label == 'admin.group.ccm.short' %}
                                                        {% if is_granted('ROLE_BASE_ADMIN_CCM_SFC_WHO_ARE_WE_ADMIN') %}
                                                            <li class="{{ app.request.attributes.get('_route') == 'ccm_sfc_who_are_we_list' ? 'active' }}">
                                                                <a data-edit="true"
                                                                   href="{{ path('ccm_sfc_who_are_we_list') }}">
                                                                    <i class="fa fa-angle-double-right"></i>
                                                                    {{ 'admin.ccm.menu.who_are_we'|trans({}, 'BaseAdminBundle') }}
                                                                </a>
                                                            </li>
                                                        {% endif %}
                                                        {% if is_granted('ROLE_BASE_ADMIN_CCM_SFC_OUR_EVENTS_ADMIN') %}
                                                            <li class="{{ app.request.attributes.get('_route') == 'ccm_sfc_our_events_list' ? 'active' }}">
                                                                <a data-edit="true"
                                                                   href="{{ path('ccm_sfc_our_events_list') }}">
                                                                    <i class="fa fa-angle-double-right"></i>
                                                                    {{ 'admin.ccm.menu.our_events'|trans({}, 'BaseAdminBundle') }}
                                                                </a>
                                                            </li>
                                                        {% endif %}
                                                        {% if is_granted('ROLE_BASE_ADMIN_CCM_SFC_RELIVE_EDITION_ADMIN') %}
                                                            <li class="{{ app.request.attributes.get('_route') == 'ccm_sfc_relive_edition_list' ? 'active' }}">
                                                                <a data-edit="true"
                                                                   href="{{ path('ccm_sfc_relive_edition_list') }}">
                                                                    <i class="fa fa-angle-double-right"></i>
                                                                    {{ 'admin.ccm.menu.relive_edition'|trans({}, 'BaseAdminBundle') }}
                                                                </a>
                                                            </li>
                                                        {% endif %}
                                                        {% if is_granted('ROLE_BASE_ADMIN_CCM_PROGRAM_ADMIN') %}
                                                            <li class="{{ app.request.attributes.get('_route') == 'admin_fdc_courtmetrage_ccmprogram_edit' ? 'active' }}">
                                                                <a data-edit="true"
                                                                   href="{{ path('admin_fdc_courtmetrage_ccmprogram_edit', {id: 1}) }}">
                                                                    <i class="fa fa-angle-double-right"></i>
                                                                    {{ 'admin.ccm.menu.program'|trans({}, 'BaseAdminBundle') }}
                                                                </a>
                                                            </li>
                                                        {% endif %}
                                                    {% endif %}
                                                    {% if group.label == 'admin.group.ccm.participer' %}
                                                        {% if is_granted('ROLE_BASE_ADMIN_CCM_FILM_REGISTER_ADMIN') %}
                                                            <li class="{{ app.request.attributes.get('_route') == 'admin_fdc_courtmetrage_ccmfilmregister_edit' ? 'active' }}">
                                                                <a data-edit="true"
                                                                   href="{{ path('admin_fdc_courtmetrage_ccmfilmregister_edit', {id: 1}) }}">
                                                                    <i class="fa fa-angle-double-right"></i>
                                                                    {{ 'admin.ccm.menu.film_register'|trans({}, 'BaseAdminBundle') }}
                                                                </a>
                                                            </li>
                                                        {% endif %}
                                                        {% if is_granted('ROLE_BASE_ADMIN_CCM_REGISTER_PROCEDURE_ADMIN') %}
                                                            <li class="{{ app.request.attributes.get('_route') == 'admin_fdc_courtmetrage_ccmregisterprocedure_list' ? 'active' }}">
                                                                <a data-edit="true"
